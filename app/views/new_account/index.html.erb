<link href='http://fonts.googleapis.com/css?family=Open+Sans' rel='stylesheet' type='text/css'>
<div class="container">
    <div class="fb-profile">
        <%= image_tag("UserPage/concert-audience.jpg", align: "left", class: "fb-image-lg", alt: "Profile image example") %> 
        <img align="left" class="fb-image-profile thumbnail" src=<%= (session[:fb_info])["image"] %> alt="Profile image example"/> 
        <div class="fb-profile-text">
            <h1><%= (session[:fb_info])["name"] %></h1>
            <p><%= (session[:fb_info])["about"] %></p>
        </div>
    </div>
</div>  


    <body id="page-top">

    <nav id="mainNav" class="navbar navbar-default navbar-fixed-top">
        <div class="container-fluid">
            <!-- Brand and toggle get grouped for better mobile display -->
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand page-scroll" href="#page-top">Audicy</a>
            </div>

            <!-- Collect the nav links, forms, and other content for toggling -->
            <div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        <a class="page-scroll" href="#contact">Contact</a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </div>
        <!-- /.container-fluid -->
    </nav>



    <header class="header-dark">
        <div class="header-content-dark">
            <div class="header-content-inner">
                <h2>Audicy</h2>
                <h4>Customized concert and music recommendations for you and your friends.</h4>
                <hr>
                <div data-bind="if: showIntro">
                    <h3 id="explanation">Tell us a bit about yourself so we can find you music youâ€™ll love</h2>

                    <button type="button" class="btn btn-success" data-bind="click: getStarted">Get Started</button>
                </div>

                <div id="question-container" data-bind="ifnot: showIntro">
                    <p id="question-text" data-bind="text: questionText"></p>

                    <div id="answer-group" data-bind="foreach: questionReplies">
                        <div class="radio">
                            <label>
                                <input class="answer-radio" type="radio" name="optradio" data-bind="click: $root.radioButtonClicked, attr: { answerindex: $index() }" >
                                <!--ko text: $data--><!--/ko-->
                            </label>
                        </div>
                    </div>

                    <button id="next-button" type="button" class="btn btn-success disabled" data-bind="click: nextButtonClicked">Next</button>

                </div>

            </div>
        </div>
    </header>

    <section id="contact" class="bg-primary">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    <h2 class="section-heading">Contact</h2>
                    <hr class="light">
                    <p>Have a question or comment? We'd love to hear from you.</p>
                </div>
                <div class="col-lg-8 col-lg-offset-2 text-center">
                    <a class="btn btn-default btn-xl" data-toggle="modal" data-target="#feedback_modal">Send Feedback</a>
                </div>
            </div>
        </div>
    </section>
    <%= render 'signup_dialog' %>
    <%= render 'feedback_dialog' %>
</body>

